{
    "files.associations": {
        "*.h": "cpp",
        "*.cpp": "cpp",
        "*.c": "cpp"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.compilerPath": "${env:DEVKITARM}/bin/arm-none-eabi-g++",
    "files.exclude": {
        "**/*.gba": false,
        "**/*.elf": true,
        "**/build": true
    }
}